#include<iostream>
#include<string>
#include<cstdio>
#include<cmath>
#include<algorithm>
#include<cstring>
using namespace std;
/*
问题描述

输入描述

输出描述

样例输入

样例输出

解题思路
归并排序
*/
const int MAXN = 1000 + 10;

int arr[MAXN];
int temp[MAXN];

void Combine(int left, int middle, int right){
    int i = left;
    int j = middle + 1;
    int k = left;;
    while(i <= middle && j <= right){
        if(arr[i] <= arr[j]){
            temp[k++] = arr[i++];
        } else {
            temp[k++] = arr[j++];
        }
    }
    while(i <= middle){
        temp[k++] = arr[i++];
    }
    while(j <= right){
        temp[k++] = arr[j++];
    }
    for(k = left;k <= right; k++){
        arr[k] = temp[k];
    }



}


void MergeSort(int left, int right){
    if(left < right){
        int middle = left + (right - left) / 2;
        MergeSort(left, middle);
        MergeSort(middle + 1, right);
        Combine(left, middle, right);
    }
}

int main(){
    int n;
    scanf("%d", &n);
    for(int i = 0; i < n; i++){
        scanf("%d", &arr[i]);
    }
    MergeSort(0, n - 1);
    for(int i = 0; i < n; i++){
        printf("%d ", arr[i]);
    }




    return 0;
}
