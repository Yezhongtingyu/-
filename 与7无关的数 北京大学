#include <iostream>
#include <cstdio>
/*
题目描述
一个正整数，如果它能被7整除，或者它的十进制表示法中某个位数上的数字为7
那么，称其为与7相关的数，现求所有小于等于n（n<100）的与7无关的正整数的平方和
例 输入21 输出2336
*/
using namespace std;

int w1(int x){	//判断是否能被7整除
    if(x % 7 == 0)
		return 1;
	else
		return 0;
}

int w2(int x){ //判断是否某个位上的数字为7
	int i,j;
	i = x % 10;
	j /= 10;
	j %= 7;
	if(i == 7 || j == 7)
		return 1;
	else
		return 0;


}

int main(){
	int m = 0;
    int in = 0;
    int n = 1;
    scanf("%d",&in);

    for( n=1;n<=in;++n){

        if( w1(n) == 0 && w2(n) == 0){
            m =  m + n*n;

        }

    }
    printf("%d\n",m);
    return 0;
}
